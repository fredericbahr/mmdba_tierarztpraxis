{
  "name": "mern-template",
  "version": "1.0.0",
  "description": "A template for a MERN-Stack Web App",
  "main": "index.js",
  "scripts": {
    "init": "./bin/init.sh",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./server/src/ ./client/src/",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./server/src/ ./client/src/ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fredericbahr/mern-template.git"
  },
  "keywords": [
    "MERN",
    "Template"
  ],
  "author": "Frederic Bahr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fredericbahr/mern-template/issues"
  },
  "homepage": "https://github.com/fredericbahr/mern-template#readme",
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^8.17.0"
  }
}
